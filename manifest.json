{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "icon_128.png",
      "default_title": "New Session Tab"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.min.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
   "description": "Log into multiple accounts on the same site simultaneously.",
   "icons": {
      "128": "icon_128.png"
   },
   "manifest_version": 2,
   "name": "NewSession",
   "permissions": [ "contextMenus", "cookies", "storage", "tabs", "http://*/*", "https://*/*", "webRequest", "webRequestBlocking", "webNavigation", "management" ],
   "version": "1.0"
}
